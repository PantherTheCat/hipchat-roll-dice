{
    "key": "hipchat-roll-dice",
    "name": "Roll Dice",
    "description": "Role some dice with /roll command.",
    "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}"
    },
    "capabilities": {
        "hipchatApiConsumer": {
            "scopes": [
                "send_notification"
            ]
        },
        "installable": {
            "callbackUrl": "{{localBaseUrl}}/installable"
        },
        "webhook": {
            "name": "roll",
            "url": "{{localBaseUrl}}/webhook",
            "pattern": "^/roll( +.*)?$",
            "event": "room_message",
            "authentication": "jwt"
        }
    }
}